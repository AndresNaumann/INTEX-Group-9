@{
    ViewData["Title"] = "Home Page";
}

@model ProjectsListViewModel

<h1>Our Products</h1>
@foreach (Project p in Model.Projects)
{
    <partial name="ProjectSummary" model="p" />
}

<div page-model="@Model.PaginationInfo" page-action="Index" page-classes-enabled="true" page-class="btn" page-class-normal="btn-outline-dark" page-class-selected="btn-primary" page-url-projectType="@Model.CurrentProjectType" class="btn-group pull-right m-1"></div>