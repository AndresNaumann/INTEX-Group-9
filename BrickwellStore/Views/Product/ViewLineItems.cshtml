@{
}
@model IEnumerable<LineItem>

<h2>Line Items for order</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.TransactionId</td>
                <td>@item.ProductId</td>
                <td>@item.ProductName</td>
                <td>@item.Quantity</td>
                <td>@item.Rating</td>
            </tr>
        }
    </tbody>
</table>