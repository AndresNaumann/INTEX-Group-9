@{
}
@model Microsoft.AspNetCore.Identity.IdentityUser
<div class="card m-1 p-1">
    <h4>
        @Model.UserName
    </h4>
    <ul>
        <li>User ID: @Model.Id</li>
        <li>Email: @Model.Email</li>
        <li>Phone Number: @Model.PhoneNumber</li>
    </ul>

    @* <a class="btn btn-info" asp-controller="Home" asp-action="EditProduct" asp-route-id="@Model.ProductId">Edit</a> *@
    <a class="btn btn-danger" asp-controller="Admin" asp-action="DeleteUser" asp-route-id="@Model.Id">Delete</a>
</div>